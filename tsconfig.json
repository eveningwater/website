{
  "compilerOptions": {
    "strictNullChecks": false,
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "jsx": "preserve",
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "noImplicitAny": false,
    "target": "es6",
    "lib": [
      "dom",
      "es2017"
    ],
    "skipLibCheck": true,
    "allowJs": true,
    "outDir": "/",
    "types": [
      "node",
      "webpack-env"
    ]
  },
  "exclude": [
    "node_modules",
    "lib",
    "es",
    "dist"
  ]
}